<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.mapper.BookMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.domain.Book">
            <id property="bookid" column="bookid" jdbcType="INTEGER"/>
            <result property="booknameCh" column="bookname_ch" jdbcType="VARCHAR"/>
            <result property="booknameEn" column="bookname_en" jdbcType="VARCHAR"/>
            <result property="author" column="author" jdbcType="VARCHAR"/>
            <result property="proName" column="pro_name" jdbcType="VARCHAR"/>
            <result property="productDate" column="product_date" jdbcType="DATE"/>
    </resultMap>

    <sql id="Base_Column_List">
        bookid,bookname_ch,bookname_en,
        author,pro_name,product_date
    </sql>
</mapper>
