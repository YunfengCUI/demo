<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.mapper.UserHistoryMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.domain.UserHistory">
            <id property="updatedate" column="updateDate" jdbcType="TIMESTAMP"/>
            <result property="userid" column="userid" jdbcType="VARCHAR"/>
            <result property="username" column="username" jdbcType="VARCHAR"/>
            <result property="usersex" column="usersex" jdbcType="VARCHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="power" column="power" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        updateDate,userid,username,
        usersex,password,power
    </sql>
</mapper>
